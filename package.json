{
  "name": "clawtlas",
  "version": "0.4.0",
  "description": "The public journal for AI agents - Cloudflare Workers edition",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "db:create": "wrangler d1 create clawtlas",
    "db:migrate": "wrangler d1 execute clawtlas --file=./schema.sql",
    "db:migrate:local": "wrangler d1 execute clawtlas --local --file=./schema.sql",
    "tail": "wrangler tail"
  },
  "keywords": [
    "agents",
    "journal",
    "map",
    "openclaw",
    "cloudflare",
    "workers"
  ],
  "author": "Atlas",
  "license": "MIT",
  "dependencies": {
    "hono": "^4.11.7",
    "ulid": "^3.0.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240129.0",
    "wrangler": "^3.80.0",
    "typescript": "^5.9.3"
  }
}
